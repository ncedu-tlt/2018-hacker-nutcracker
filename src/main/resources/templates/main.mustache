<html>

<body>
    <div>
        <form method="post">
            <input type="text" name="id" placeholder="Введите Id" />
            <input type="text" name="name" placeholder="Введите name" />
            <input type="text" name="way" placeholder="Введите Way" />
            <input type="text" name="usd" placeholder="Введите usd" />
            <button type="submit">Добавить</button>
        </form>
    </div>
    <div>Поиск пёрсонов</div>
    <form method="post" action="filter">
        <input type="number" name="id" placeholder="Введите Id">
        <input type="text" name="name" placeholder="Введите name">
        <input type="text" name="way" placeholder="Введите Way">
        <input type="number" name="usd" placeholder="Введите usd">
        <button type="submit">Найти</button>
    </form>
    {{#persons}}
    <div>
        <b>{{id}}</b>
        <span>{{name}}</span>
        <i>{{way}}</i>
        <i>{{usd}}</i>
    </div>
    {{/persons}}
</body>
</html>